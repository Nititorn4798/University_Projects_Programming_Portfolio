    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="bmi" type="Real" array="False" size=""/>
            <output expression="&quot;Input Your BMI : &quot;" newline="True"/>
            <input variable="bmi"/>
            <if expression="bmi &gt;= 30">
                <then>
                    <output expression="&quot;You are dangerous obesity&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="bmi &lt;= 29.9 &amp;&amp; bmi &gt;= 25">
                        <then>
                            <output expression="&quot;You are obesity&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="bmi &lt;= 24.9 &amp;&amp; bmi &gt;= 23">
                                <then>
                                    <output expression="&quot;You are overweight&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="bmi &lt;= 22.9 &amp;&amp; bmi &gt;= 18.5">
                                        <then>
                                            <output expression="&quot;You are slim&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="bmi &lt;= 18.4 &amp;&amp; bmi &gt;= 10">
                                                <then>
                                                    <output expression="&quot;You are underweight&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="bmi &lt; 10">
                                                        <then>
                                                            <output expression="&quot;ARE YOU KIDDING ME?&quot;" newline="True"/>
                                                            <output expression="&quot;Please Check Your Input BMI&quot;" newline="True"/>
                                                        </then>
                                                        <else/>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
